    <sql>-- This script helps to change the wide format data to long format reducing the many number of columns to one column.

SELECT * FROM 
(

--q11_fever

SELECT 1 as condition_occurrence_id, person_id, 
case
	when q11_fever = 'yes' then 437663
	else 0
end as condition_concept_id,
created_date as condition_start_date, 
created_date as condition_start_timestamp,
created_date as condition_end_date, 
created_date as condition_end_timestamp,
4303401 as condition_type_concept_id,
603082 as condition_status_concept_id,
NULL as stop_reason,
provider_id,
1 as visit_occurrence_id,
0 as visit_detail_id,
case
	when q11_fever = 'yes' then 'Fever : Yes'
	else 'No'
end as condition_source_value,
0 as condition_source_concept_id,
NULL as condition_status_source_value

FROM (SELECT * FROM staging.serodata_two order by studyid ASC) AS A

UNION ALL
--q11_shortness_breath

SELECT 1 as condition_occurrence_id, person_id, 
case
	when q11_shortness_breath = 'yes' then 4308377
